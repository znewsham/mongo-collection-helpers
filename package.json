{
  "name": "mongo-collection-helpers",
  "version": "1.0.8",
  "description": "A package to provide helper functionality for other packages that need mongo projections and test helpers",
  "type": "module",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "build": "npx tsc",
    "test": "node --test test/index.js",
    "test:watch": "node --test --watch test/index.js",
    "prepublishOnly": "npm run build && npm test"
  },
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "default": "./lib/index.js"
    },
    "./testHelpers": "./testHelpers.js",
    "./testHelpers.js": "./testHelpers.js"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@blastjs/minimongo": "^0.1.0",
    "mongodb": "^5.9.2",
    "typescript": "^5.4.2"
  }
}
